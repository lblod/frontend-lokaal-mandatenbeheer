{{page-title "Instances"}}

<InstanceTableConfiguration @labels={{this.model.headerLabels}} />
<div class="au-c-body-container au-c-body-container--scroll">
  <SemanticForms::InstanceTable
    @sort={{this.sort}}
    @page={{this.page}}
    @size={{this.size}}
    @filter={{this.filter}}
    @search={{this.search}}
    @formDefinition={{this.model.formDefinition}}
    @onRemoveInstance={{this.onRemoveInstance}}
    @editRoute="forms.form.instance"
  />
</div>