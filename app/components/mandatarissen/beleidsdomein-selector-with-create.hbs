<Mandatarissen::ConceptSelectorWithCreate
  @multiple={{true}}
  @type="beleidsdomein-code"
  @conceptScheme={{this.conceptScheme}}
  @search={{this.search}}
  @searchEnabled={{true}}
  @selected={{this.selected}}
  @options={{this.options}}
  @onChange={{this.select}}
  @onCreate={{this.add}}
  @disabled={{@disabled}}
  as |concept|
>
  {{concept.label}}
</Mandatarissen::ConceptSelectorWithCreate>