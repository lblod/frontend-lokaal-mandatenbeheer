<AuButton
  @skin="link"
  @icon="plus"
  {{on "click" (fn (mut this.showModal) true)}}
  class="au-u-padding-left-none au-u-margin-bottom"
>
  Voeg een veld toe
</AuButton>

{{yield}}

<RdfInputFields::CrudCustomFieldModal
  @isCreating={{true}}
  @showModal={{this.showModal}}
  @onCloseModal={{fn (mut this.showModal) false}}
  @form={{@form}}
  @field={{@field}}
/>