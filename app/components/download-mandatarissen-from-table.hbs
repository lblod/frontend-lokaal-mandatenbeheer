<Shared::Tooltip
  @showTooltip={{true}}
  @tooltipText="Exporteer mandatarissen naar CSV formaat"
>
  <AuLinkExternal
    href={{this.downloadLink}}
    @disabled={{(or (not @bestuursperiode) this.prepareDownloadLink.isRunning)}}
    download="mandatarissen"
    @skin="button-secondary"
    @loading={{this.prepareDownloadLink.isRunning}}
  >Download</AuLinkExternal>
</Shared::Tooltip>